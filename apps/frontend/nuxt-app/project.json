{
  "name": "nuxt-app",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/frontend/nuxt-app",
  "projectType": "application",
  "tags": [
    "type:application",
    "scope:frontend"
  ],
  "implicitDependencies": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nuxt build",
        "cwd": "apps/frontend/nuxt-app"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nuxt dev",
        "cwd": "apps/frontend/nuxt-app"
      }
    },
    "generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nuxt generate",
        "cwd": "apps/frontend/nuxt-app"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nuxt preview",
        "cwd": "apps/frontend/nuxt-app"
      },
      "dependsOn": [
        "build"
      ]
    }
  }
}
